CREATE TABLE `personal` (
  `name` varchar(40) NOT NULL,
  `admnno` varchar(12) NOT NULL,
  `dob` date DEFAULT NULL,
  `nationality` varchar(20) DEFAULT NULL,
  `gender` varchar(10) DEFAULT NULL,
  `email` varchar(30) DEFAULT NULL,
  `skypeid` varchar(20) DEFAULT NULL,
  `mobno` varchar(15) DEFAULT NULL,
  `category` varchar(10) DEFAULT NULL,
  `paddress` varchar(80) DEFAULT NULL,
  `caddress` varchar(80) DEFAULT NULL,
  `xyear` year(4) DEFAULT NULL,
  `xboard` varchar(10) DEFAULT NULL,
  `xpercgpa` decimal(3,2) DEFAULT NULL,
  `xiiyear` year(4) DEFAULT NULL,
  `xiiboard` varchar(10) DEFAULT NULL,
  `xiipercgpa` decimal(5,2) DEFAULT NULL,
  `gyear` year(4) DEFAULT NULL,
  `gboard` varchar(30) DEFAULT NULL,
  `gpercgpa` decimal(3,2) DEFAULT NULL,
  `xp` varchar(200) DEFAULT NULL,
  `achievements` varchar(200) DEFAULT NULL,
  `skills` varchar(80) DEFAULT NULL,
  `interests` varchar(75) DEFAULT NULL,
  `aoi` varchar(50) DEFAULT NULL,
  PRIMARY KEY (`admnno`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;